@using System.Web.UI.WebControls
@model Vidly.Models.Customer
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@if (@Model.Name !=null)
{
    <h2>@Model.Name</h2>

    <p>MembershipType : @Model.MembershipType.Name</p>
    if ( @Model.Birthday != null)
    {
        <p>Birth day : @Model.Birthday.Value.Date.ToString("yyyy MMMM dd")</p>
    }

}
else
{
    <h2></h2>

    <p>Nu mai exista </p>
}
